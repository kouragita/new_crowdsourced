      "icons": [
        {
          "src": "shortcut-dashboard.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Leaderboard",
      "short_name": "Leaderboard",
      "description": "View the learning leaderboard",
      "url": "/dashboard/leaderboard",
      "icons": [
        {
          "src": "shortcut-leaderboard.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Calendar",
      "short_name": "Calendar",
      "description": "Open your learning calendar",
