        {
          "src": "shortcut-calendar.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Courses",
      "short_name": "Courses",
      "description": "Browse available courses",
      "url": "/dashboard/courses",
      "icons": [
        {
          "src": "shortcut-courses.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
